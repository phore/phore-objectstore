{
  "name": "phore/objectstore",
  "description": "Object store for local files, google, aws",
  "license": "MIT",
  "authors": [
    { "name": "Matthias Leuffen", "email": "m@tth.es" }
  ],
  "repositories": [
  ],
  "require": {
    "php": ">=8.0",
    "phore/core": "*",
    "ext-json": "*",
    "ext-yaml": "*",
    "phore/http-client": "*",
    "ext-openssl": "*"
  },
  "suggest": {
    "google/cloud-storage": "If using Google Cloud services",
    "phore/filesystem": "If using filesystem driver",
    "microsoft/azure-storage-blob": "Access Azure"
  },
  "autoload": {
    "psr-4": {
      "Phore\\ObjectStore\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "require-dev": {
    "phore/tester": "dev-master",
    "lack/keystore": "dev-main",
    "google/cloud-storage": "*",
    "phore/filesystem": "*",
    "aws/aws-sdk-php": "^3.300.7",
    "microsoft/azure-storage-blob": "*"
  },
  "config": {
    "preferred-install": "source",
    "allow-plugins": {
      "php-http/discovery": false
    }
  }
}
